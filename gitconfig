[core]
	excludesfile = ~/.gitignore
	quotepath = false
	editor = vim

[alias]
	st = status
	ci = commit
	co = checkout
	br = branch
	cp = cherry-pick
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative --all
	f = fetch
	d = diff
	ru = remote update

[color]
	ui = auto

[user]
	name = Charles Lehner
	email = celehner1@gmail.com

[branch "master"]
	merge = refs/heads/master
	remote = origin
[push]
	default = matching
